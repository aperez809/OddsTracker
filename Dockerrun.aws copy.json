{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "dockerLabels": {
                "com.github.jrcs.letsencrypt_nginx_proxy_companion.nginx_proxy": null
            },
            "essential": true,
            "image": "013206471204.dkr.ecr.us-east-2.amazonaws.com/oddstracker-staging:nginx-proxy",
            "mountPoints": [
                {
                    "containerPath": "/etc/nginx/certs",
                    "sourceVolume": "certs"
                },
                {
                    "containerPath": "/usr/share/nginx/html",
                    "sourceVolume": "html"
                },
                {
                    "containerPath": "/etc/nginx/vhost.d",
                    "sourceVolume": "vhost"
                },
                {
                    "containerPath": "/tmp/docker.sock",
                    "sourceVolume": "VarRunDocker_Sock"
                }
            ],
            "name": "nginx-proxy",
            "portMappings": [
                {
                    "containerPort": 443,
                    "hostPort": 443
                },
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        },
        {
            "environment": [
                {
                  "name": "seconds_to_wait",
                  "value": 2
                }
              ],
            "essential": true,
            "image": "jrcs/letsencrypt-nginx-proxy-companion",
            "mountPoints": [
                {
                    "containerPath": "/var/run/docker.sock",
                    "sourceVolume": "VarRunDocker_Sock"
                },
                {
                    "containerPath": "/etc/nginx/certs",
                    "sourceVolume": "certs"
                },
                {
                    "containerPath": "/usr/share/nginx/html",
                    "sourceVolume": "html"
                },
                {
                    "containerPath": "/etc/nginx/vhost.d",
                    "sourceVolume": "host"
                }
            ],
            "name": "nginx-proxy-letsencrypt"
        },
        {
            "command": [
                "gunicorn",
                "oddstracker_admin.wsgi:application",
                "--bind",
                "0.0.0.0:8000"
            ],
            "essential": true,
            "image": "013206471204.dkr.ecr.us-east-2.amazonaws.com/oddstracker-staging:web",
            "name": "web"
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "certs"
            },
            "name": "Certs"
        },
        {
            "host": {
                "sourcePath": "html"
            },
            "name": "Html"
        },
        {
            "host": {
                "sourcePath": "vhost"
            },
            "name": "Vhost"
        },
        {
            "host": {
                "sourcePath": "/var/run/docker.sock"
            },
            "name": "VarRunDocker_Sock"
        }
    ]
}

{
    "containerDefinitions": [
        {
            "command": [
                "gunicorn",
                "oddstracker_admin.wsgi:application",
                "--bind",
                "0.0.0.0:8000"
            ],
            "essential": true,
            "image": "013206471204.dkr.ecr.us-east-2.amazonaws.com/oddstracker-staging:web",
            "name": "web"
        },
        {
            "essential": true,
            "image": "013206471204.dkr.ecr.us-east-2.amazonaws.com/oddstracker-staging:nginx-proxy",
            "mountPoints": [
                {
                    "containerPath": "/etc/nginx/certs",
                    "sourceVolume": "Certs"
                },
                {
                    "containerPath": "/usr/share/nginx/html",
                    "sourceVolume": "Html"
                },
                {
                    "containerPath": "/etc/nginx/vhost.d",
                    "sourceVolume": "Vhost"
                },
                {
                    "containerPath": "/tmp/docker.sock",
                    "sourceVolume": "VarRunDocker_Sock"
                }
            ],
            "name": "nginx-proxy",
            "portMappings": [
                {
                    "containerPort": 443,
                    "hostPort": 443
                },
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "certs"
            },
            "name": "Certs",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true
            }
        },
        {
            "host": {
                "sourcePath": "html"
            },
            "name": "Html",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true
            }
        },
        {
            "host": {
                "sourcePath": "vhost"
            },
            "name": "Vhost",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true
            }
        },
        {
            "host": {
                "sourcePath": "/var/run/docker.sock"
            },
            "name": "VarRunDocker_Sock",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true
            }
        }

    ]
}